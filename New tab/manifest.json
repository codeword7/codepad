{
  "name": "CodePad",
  "short_name": "Pad",
  "version": "1.1",
  "manifest_version": 2,
  "description": "A whole new way to use your Chrome Tab. Open a new tab to take notes.",
  "icons": {
    "18": "img/icon/icon18.png",
    "48": "img/icon/icon48.png",
    "128": "img/icon/icon128.png"
  },
  "background": {
    "scripts": [
      "js/bundle.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "fontSettings"
  ]
}
